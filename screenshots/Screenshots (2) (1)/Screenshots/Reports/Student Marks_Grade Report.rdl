  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>FLEX</DataSourceName>
        <CommandText>SELECT s.REG_NUM, s.STUDENT_NAME, c.COURSE_CODE, m.MARKS, CASE 
    WHEN m.MARKS &gt;= 90 THEN 'A+' 
    WHEN m.MARKS &gt;= 80 AND m.MARKS &lt; 90 THEN 'A' 
    WHEN m.MARKS &gt;= 70 AND m.MARKS &lt; 80 THEN 'B' 
    WHEN m.MARKS &gt;= 60 AND m.MARKS &lt; 70 THEN 'C' 
    WHEN m.MARKS &gt;= 50 AND m.MARKS &lt; 60 THEN 'D' 
    ELSE 'F' 
  END AS GRADE
FROM STUDENTS s
JOIN MARKS m ON s.REG_NUM = m.REG_NUM
JOIN COURSES c ON m.COURSE_CODE = c.COURSE_CODE;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
