  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>FLEX</DataSourceName>
        <CommandText>SELECT s.REG_NUM, s.STUDENT_NAME, 
    COUNT(CASE WHEN a.A_STATUS IN ('P', 'L') THEN 1 END) / COUNT(*) * 100 AS ATTENDANCE_PERCENTAGE
FROM STUDENTS s
INNER JOIN REGISTRATION r ON s.REG_NUM = r.REG_NUM
INNER JOIN SECTIONS sec ON r.SECTION_ID = sec.SECTION_ID
LEFT JOIN ATTENDANCE a ON sec.SECTION_ID = a.SECTION_ID AND r.REG_NUM = a.STUDENT_ID
GROUP BY s.REG_NUM, s.STUDENT_NAME;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="REG_NUM">
          <DataField>REG_NUM</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="STUDENT_NAME">
          <DataField>STUDENT_NAME</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ATTENDANCE_PERCENTAGE">
          <DataField>ATTENDANCE_PERCENTAGE</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
